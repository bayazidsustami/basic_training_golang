{
    "openapi" : "3.0.3",
    "info": {
        "title":"Category RestFull Api",
        "description": "Api Spec For category RestFull Api",
        "version": "1.0.0"
    },
    "servers": [
        {
            "url":"http://localhost:3000/api"
        }
    ],
    "paths":{
        "/categories" : {
            "get": {
                "description": "List All Categories",
                "responses" :{
                    "200" : {
                        "description": "Success get all categories",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "code" : {
                                            "type": "number"
                                        },
                                        "status": {
                                            "type": "string"
                                        },
                                        "data": {
                                            "type": "array",
                                            "items": {
                                                "type": "object",
                                                "properties": {
                                                    "id": {
                                                        "type":"number"
                                                    },
                                                    "name": {
                                                        "type":"string"
                                                    }
                                                }
                                            }
                                        }
                                    }

                                }
                            }
                        }
                    }
                }
            }
        }
    }
}